{
  "name": "kontui",
  "private": true,
  "version": "0.0.1",
  "workspaces": [
    "packages/*"
  ],
  "description": "",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "clean": "lerna clean --yes && rm -rf node_modules",
    "clean:dist": "pnpm lerna exec -- rimraf dist",
    "lerna": "lerna",
    "prestart": "pnpm bootstrap",
    "boot": "pnpm prestart && pnpm build",
    "start": "pnpm build && pnpm storybook",
    "start:all": "pnpm build && concurrently  \"pnpm storybook\" ",
    "build": "turbo run build",
    "bootstrap": "pnpm install && lerna bootstrap --use-workspaces",
    "test": "turbo run test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "pkg": "manypkg run",
    "fix:pkgs": "manypkg fix",
    "changeset": "changeset",
    "version:pkgs": "changeset version",
    "release": "changeset publish",
    "changelog:gen": "ts-node scripts/changelog-generate.ts",
    "changelog:write": "ts-node scripts/changelog-write.ts",
    "changelog:commit": "git add CHANGELOG.md && git commit -am 'docs: add changelog' && git push",
    "format": "prettier -w .",
    "check": "prettier -C",
    "prelint": "pnpm format",
    "lint": "concurrently pnpm:lint:*",
    "lint:eslint": "eslint packages/**/src/ --ext .ts,.tsx,.js,.jsx --config .eslintrc",
    "lint:types": "tsc --noEmit",
    "lint-fix": "pnpm lint --fix",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "author": "karyanayandi <karyana@dafunda.io>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dafundacom/kontui"
  },
  "bugs": {
    "url": "https://github.com/dafundacom/kontui/issues"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,html,css,json}": [
      "pnpm prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.18.6",
    "@changesets/changelog-github": "^0.4.5",
    "@changesets/cli": "^2.23.0",
    "@changesets/get-release-plan": "^3.0.9",
    "@changesets/types": "^5.0.0",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@commitlint/cz-commitlint": "^17.0.3",
    "@manypkg/cli": "^0.19.1",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/addon-storysource": "^6.5.9",
    "@storybook/addon-toolbars": "^6.5.9",
    "@storybook/builder-webpack4": "^6.5.9",
    "@storybook/manager-webpack4": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@types/edit-json-file": "^1.7.0",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.182",
    "@types/prettier": "^2.6.3",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "babel-loader": "^8.2.5",
    "commitizen": "^4.2.4",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "edit-json-file": "^1.7.0",
    "eslint": "8.19.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "5.5.1",
    "glob": "^8.0.3",
    "husky": "^8.0.1",
    "lerna": "^5.1.6",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "prettier": "^2.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "storybook-addon-performance": "^0.16.1",
    "styled-jsx": "^5.0.2",
    "ts-node": "^10.8.2",
    "turbo": "^1.3.1",
    "typescript": "^4.7.4"
  }
}
